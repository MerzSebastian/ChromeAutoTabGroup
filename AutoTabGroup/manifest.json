{
  "name": "AutoTabGroup",
  "version": "1.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
      "tabs",
      "tabGroups",
      "contextMenus",
      "storage"
  ],
  "icons": {
    "16": "icons/black/16.png",
    "24": "icons/black/24.png",
    "32": "icons/black/32.png",
    "48": "icons/black/48.png",
    "64": "icons/black/64.png",
    "72": "icons/black/72.png",
    "80": "icons/black/80.png",
    "96": "icons/black/96.png",
    "128": "icons/black/128.png"
  },
  "action": {}
}
